$$if not CLEAN_RESET_ICE then
$$  CLEAN_RESET_ICE = 1

unit clean_reset(
  output uint1 out(1)
) {
  uint4  trigger = 15;
  always {
    out     = trigger[0,1];
    trigger = trigger >> 1;
  }
}

$$end
